   "source": [
    "import os\n",
    "import re\n",
    "\n",
    "path = 'rename/' # 경로 수정\n",
    "\n",
    "for n,filename in enumerate(os.listdir(path)):\n",
    "    n+=1\n",
    "    # 확장자\n",
    "    ex = re.findall('\\..+', filename)[0]\n",
    "    \n",
    "    os.rename(path+filename, path+'pen_'+ str(n) + ex) # 'pen_' 부분 수정\n",
    "print(os.listdir(path))"
   ]
